{
	"name": "@suhomozgy-andrey/panthera-tailwind",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/suhomozgy-andrey"
	},
	"version": "0.0.47",
	"description": "A simple template for a custom React component library",
	"scripts": {
		"test": "jest",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"build": "NODE_ENV=production rollup -c && npm run build:es",
		"build:es": "npm run build:es-icons",
		"build:es-icons": "NODE_ENV=production tsc -p tsconfig-icons.json",
		"prepare-icons": "npx @svgr/cli --typescript --out-dir src/components/Icons -- src/assets/icons",
		"build-analyze": "NODE_ENV=analyze npm run rollup -c",
		"lint": "npm run eslint && npm run stylelint",
		"eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"eslint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
		"stylelint": "stylelint 'src/**/*.{less,css}'",
		"stylelint:fix": "stylelint 'src/**/*.{less,css}' --fix",
		"lint-fix": "npm run eslint:fix && npm run stylelint:fix",
		"check-types": "tsc"
	},
	"author": "Andrei Sukhomozgii",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.17.12",
